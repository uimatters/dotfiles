[credential]
	helper = osxkeychain
[user]
	email = corey.reed@tcu.edu
	name = Corey Reed
[merge]
	ff = only
[fetch]
	prune = true
[commit]
	template = /Users/coreyreed/Documents/git-commit-tmpl.txt
[alias]
	sla = log --oneline --decorate --graph --all
	glog = log -E -i --grep
	uncommit = reset --soft HEAD^
	unstage = reset
	co = checkout
[core]
	autocrlf = input
	editor = code --wait
[pull]
	rebase = false
[push]
  default = upstream